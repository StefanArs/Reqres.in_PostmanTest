		{
			"name": "Create Morpheus User",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const response = pm.response.json();\r",
							"\r",
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"pm.test(\"The name has to be morpheus\", function () {\r",
							"    pm.expect(response.name).to.eql(\"john\");\r",
							"});\r",
							"pm.test(\"Job title 'leader'\", function () {\r",
							"    pm.expect(response.job).to.eql(\"leader\");\r",
							"});\r",
							"pm.environment.set(\"Id_Morpheus\", response.id);\r",
							"\r",
							"pm.test(\"Check the Morpheus's Id\", function () {\r",
							"    pm.expect(response.id).to.eql(pm.environment.get(\"Id_Morpheus\"));\r",
							"});\r",
							"pm.test(\"The current timestamp\", function () {\r",
							"    pm.expect(response.date).to.eql(pm.environment.get(\"currentdate\"));\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript"
