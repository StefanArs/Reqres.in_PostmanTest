					"listen": "test",
					"script": {
						"exec": [
							"const response = pm.response.json();\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Total number of pages should be 2\", function () {\r",
							"    pm.expect(response.total_pages).to.eql(2);\r",
							"});\r",
							"\r",
							"pm.test(\"Tobias's email address is 'tobias.funke@reqres.in'\", function () {\r",
							"    pm.expect(response.data[2].email).to.eql(\"tobias.funke@reqres.in\");\r",
							"});\r",
							"let name;\r",
							"\r",
							"for (let lastname of response.data) {\r",
							"    if (lastname.last_name === \"Ferguson\") {\r",
							"        name = lastname;\r",
							"    }\r",
							"}\r",
							"pm.test(\"The Last Name is Ferguson\", function () {\r",
							"    pm.expect(name.last_name).to.eql(\"Ferguson\");\r",
							"});\r",
